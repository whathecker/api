{
    "AWSEBDockerrunVersion": 2,
    "Authentication": {
        "Bucket": "elasticbeanstalk-eu-west-3-243549454379",
        "Key": "docker-auth/.dockercfg"
    },
    "containerDefinitions": [
      {
        "name": "nodeapp",
        "image": "hellochokchok/backend:latest",
        "essential": true,
        "memoryReservation": 400
      },
      {
          "name": "rabbitmq",
          "image": "rabbitmq",
          "essential": true,
          "links": [
              "nodeapp"
          ],
          "memoryReservation": 128
      },
      {
          "name": "start_dependencies",
          "image": "dadarek/wait-for-dependencies",
          "essential": true,
          "memory": 50
      }
    ]
  }